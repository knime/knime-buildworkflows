<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<fragment>
	<extension point="org.knime.workbench.repository.categories">
		<category
			description="Nodes that capture and write workflows for continuous and Integrated Deployment."
			icon="icons/capture.png" level-id="captureworkflows"
			name="Integrated Deployment" path="/flowabstraction">
		</category>
	</extension>
 <extension
       point="org.knime.workbench.repository.nodes">
    <node
          category-path="/flowabstraction/captureworkflows"
          factory-class="org.knime.buildworkflows.capture.start.CaptureWorkflowStartNodeFactory">
    </node>
    <node
          after="org.knime.buildworkflows.capture.start.CaptureWorkflowStartNodeFactory"
          category-path="/flowabstraction/captureworkflows"
          factory-class="org.knime.buildworkflows.capture.end.CaptureWorkflowEndNodeFactory">
    </node>
    <node
          after="org.knime.buildworkflows.capture.end.CaptureWorkflowEndNodeFactory"
          category-path="/flowabstraction/captureworkflows"
          factory-class="org.knime.buildworkflows.combiner.WorkflowCombinerNodeFactory">
    </node>
    <node
          after="org.knime.buildworkflows.combiner.WorkflowCombinerNodeFactory"
          category-path="/flowabstraction/captureworkflows"
          deprecated="false"
          factory-class="org.knime.buildworkflows.reader.WorkflowReaderNodeFactory">
    </node>
    <node
          after="org.knime.buildworkflows.reader.WorkflowReaderNodeFactory"
          category-path="/flowabstraction/captureworkflows"
          deprecated="true"
          factory-class="org.knime.buildworkflows.writer.WorkflowWriterNodeFactory">
    </node>
    <node
          after="org.knime.buildworkflows.reader.WorkflowReaderNodeFactory"
          category-path="/flowabstraction/captureworkflows"
          factory-class="org.knime.buildworkflows.writer.WorkflowWriter2NodeFactory">
    </node>
    <node
          after="org.knime.buildworkflows.writer.WorkflowWriter2NodeFactory"
          category-path="/flowabstraction/captureworkflows"
          deprecated="true"
          factory-class="org.knime.buildworkflows.deploy.DeployWorkflowNodeFactory">
    </node>
    <node
          after="org.knime.buildworkflows.writer.WorkflowWriter2NodeFactory"
          category-path="/flowabstraction/captureworkflows"
          deprecated="true"
          factory-class="org.knime.buildworkflows.deploy.DeployWorkflow2NodeFactory">
    </node>
    <node
          after="org.knime.buildworkflows.writer.WorkflowWriter2NodeFactory"
          category-path="/flowabstraction/captureworkflows"
          deprecated="false"
          factory-class="org.knime.buildworkflows.deploy.DeployWorkflow3NodeFactory"
          hidden="false">
    </node>
    <node
          after="org.knime.buildworkflows.deploy.DeployWorkflow3NodeFactory"
          category-path="/flowabstraction/captureworkflows"
          factory-class="org.knime.buildworkflows.executor.WorkflowExecutorNodeFactory">
    </node>
    <node
          category-path="/flowabstraction/captureworkflows"
          factory-class="org.knime.buildworkflows.converter.WorkflowConverterNodeFactory">
    </node>
    <node
       category-path="/flowabstraction/captureworkflows"
       deprecated="false"
       factory-class="org.knime.buildworkflows.summaryextractor.WorkflowSummaryExtractorNodeFactory"
       hidden="false">
 </node>
 </extension>
</fragment>
